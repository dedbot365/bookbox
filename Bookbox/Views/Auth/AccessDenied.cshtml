@{
    ViewData["Title"] = "Access Denied";
}

<div class="container mt-5">
    <div class="text-center">
        <h1 class="text-danger">Access Denied</h1>
        <p class="lead">You do not have permission to access this resource.</p>
        <hr />
        <p>
            <a href="javascript:history.back()" class="btn btn-outline-primary me-2">Go Back</a>
            <a asp-controller="Auth" asp-action="Login" class="btn btn-primary">Login</a>
        </p>
    </div>
</div>